{
  "expo": {
    "name": "RePath",
    "slug": "repath",
    "version": "0.0.1",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "react-native-fast-tflite",
        {
          "enableAndroidGpuLibraries": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "Allow RePath to use your camera to scan items."
      },
      "bundleIdentifier": "com.anonymous.repath"
    },
    "android": {
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.anonymous.repath",
      "minSdkVersion": 26
    },
    "icon": "./assets/icon.png"
  }
}
